{
  "name": "razroo-vscode-plugin",
  "displayName": "Razroo VSCode Plugin",
  "description": "The complementary VSCode plugin for the Razroo application.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:razroo-vscode-plugin.initialization",
    "onCommand:razroo-vscode-plugin.authenticateViaWebview",
    "onCommand:extension.getGitHubUser",
    "onCommand:extension.auth0Authentication"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "razroo-vscode-plugin.initialization",
        "title": "Razroo Initialization"
      },
      {
        "command": "razroo-vscode-plugin.authenticateViaWebview",
        "title": "Razroo Authentication"
      },
      {
        "command": "extension.getGitHubUser",
        "title": "Razroo Github Authentication"
      },
      {
        "command": "extension.auth0Authentication",
        "title": "Razroo Auth0 Authentication"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "scope=DEVELOPMENT tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "open": "^8.2.1",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
