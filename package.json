{
  "name": "razroo-vscode-plugin",
  "publisher": "Razroo",
  "displayName": "razroo-vscode-plugin",
  "description": "The complementary VSCode plugin for the Razroo application.",
  "version": "1.6.5",
  "private": true,
  "engines": {
    "vscode": "^1.57.0"
  },
  "icon": "media/razroo-icon.jpg",
  "main": "./dist/extension",
  "galleryBanner": {
    "color": "#1d2f42",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/razroo/razroo-vscode-plugin"
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "esm": "esm",
    "webpack-watch": "webpack --mode development --watch",
    "package": "webpack --devtool hidden-source-map",
    "compile": "webpack",
    "publish": "vsce publish",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test-compile": "tsc -p ./"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.auth0Authentication",
        "title": "Razroo Auth0 Authentication"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "razroo-bar",
          "title": "Razroo",
          "icon": "media/razroo-logo.svg"
        }
      ]
    },
    "views": {
      "razroo-bar": [
        {
          "id": "razroo-authentication",
          "name": "Authentication"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "razroo-authentication",
        "contents": "Razroo Extension initializing...",
        "when": "!razroo-vscode-plugin:activated"
      },
      {
        "view": "razroo-authentication",
        "contents": "Click on the \"GO!\" button to connect your VS Code Editor to Razroo.\n[Go!](command:extension.auth0Authentication)",
        "when": "razroo-vscode-plugin:activated && !razroo-vscode-plugin:isAuthenticated && !razroo-vscode-plugin:isAuthenticationInProgress && !razroo-vscode-plugin:isAuthenticationCancelling"
      },
      {
        "view": "razroo-authentication",
        "contents": "VSCode Authentication Cancelling In Progress...",
        "when": "razroo-vscode-plugin:isAuthenticationCancelling"
      },
      {
        "view": "razroo-authentication",
        "contents": "VSCode Authentication In Progress...",
        "when": "razroo-vscode-plugin:isAuthenticationInProgress"
      },
      {
        "view": "razroo-authentication",
        "contents": "Click on the \"Cancel!\" button to cancel authentication.\n[Cancel!](command:extension.cancelAuthentication)",
        "when": "razroo-vscode-plugin:isAuthenticationInProgress"
      },
      {
        "view": "razroo-authentication",
        "contents": "You're all set! Push production code to VS Code from the Razroo web application.\n[Logout](command:extension.logout)",
        "when": "razroo-vscode-plugin:isAuthenticated"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@octokit/rest": "^19.0.4",
    "@types/adm-zip": "^0.5.0",
    "@types/auth0": "^2.34.9",
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/filesystem": "0.0.32",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "18.8.4",
    "@types/parse-git-config": "^3.0.1",
    "@types/request": "^2.48.7",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vscode/test-electron": "^2.1.5",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "chai": "^4.3.4",
    "esbuild": "^0.15.10",
    "eslint": "^8.25.0",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "glob": "^8.0.3",
    "jest": "^29.2.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "resolve-typescript-plugin": "^1.1.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "vscode-test": "^1.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^14.2.5",
    "@aws-sdk/signature-v4-crt": "^3.45.0",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@razroo/razroo-codemod": "^1.7.4",
    "@types/jsonwebtoken": "^8.5.5",
    "adm-zip": "^0.5.5",
    "auth0": "^2.37.0",
    "aws-appsync": "^4.1.8",
    "aws-appsync-subscription-link": "^3.0.9",
    "aws-sdk": "2.1231.0",
    "axios": "^1.1.3",
    "child_process": "^1.0.2",
    "cookiejar": "^2.1.3",
    "cors": "^2.8.5",
    "es6-promise": "^4.2.8",
    "express": "^4.18.2",
    "file-system": "^2.2.2",
    "follow-redirects": "^1.14.4",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "git-branch": "^2.0.1",
    "globify-gitignore": "^0.2.1",
    "graphql": "^15.6.0",
    "graphql-tag": "2.11.0",
    "ignore": "^5.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "merge-options": "^3.0.4",
    "mime": "^3.0.0",
    "mocha": "^10.0.0",
    "open": "^8.2.1",
    "parse-git-config": "^3.0.0",
    "parse-gitignore": "^2.0.0",
    "prettier": "^2.5.1",
    "request": "^2.88.2",
    "resolve": "^1.21.0",
    "subscriptions-transport-ws": "^0.11.0",
    "superagent-proxy": "^3.0.0",
    "ts-loader": "^9.4.1",
    "uuid": "^9.0.0",
    "ws": "^8.2.0"
  }
}
