{
  "name": "razroo-vscode-plugin",
  "displayName": "Razroo VSCode Plugin",
  "description": "The complementary VSCode plugin for the Razroo application.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:razroo-vscode-plugin.initialization",
    "onCommand:razroo-vscode-plugin.authenticate",
    "onCommand:extension.getGitHubUser"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "razroo-vscode-plugin.initialization",
        "title": "Razroo Initialization"
      },
      {
        "command": "razroo-vscode-plugin.authenticate",
        "title": "Razroo Authentication"
      },
      {
        "command": "extension.getGitHubUser",
        "title": "Razroo Github Authentication"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.57.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "@octokit/rest": "^18.0.0"
  }
}
