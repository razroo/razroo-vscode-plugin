{
  "name": "razroo-vscode-plugin",
  "publisher": "Razroo",
  "displayName": "razroo-vscode-plugin",
  "description": "The complementary VSCode plugin for the Razroo application.",
  "version": "0.6.7",
  "engines": {
    "vscode": "^1.58.0"
  },
  "galleryBanner": {
    "color": "#1d2f42",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/razroo/razroo-vscode-plugin"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile": "webpack --mode production",
    "publish": "vsce publish --no-dependencies",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-compile": "tsc -p ./"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.auth0Authentication"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.auth0Authentication",
        "title": "Razroo Auth0 Authentication"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "razroo-bar",
          "title": "Razroo",
          "icon": "media/razroo-logo.svg"
        }
      ]
    },
    "views": {
      "razroo-bar": [
        {
          "id": "razroo-authentication",
          "name": "Authentication"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "razroo-authentication",
        "contents": "Click on the \"Authenticate\" button to connect your VS Code to Razroo.\n[Authenticate](command:extension.auth0Authentication)",
        "when": "!authenticate"
      },
      {
        "view": "razroo-authentication",
        "contents": "You're all set! Push production code to VS Code from the Razroo web application.\n[Authenticated](command:extension.auth0Authentication)",
        "when": "authenticate",
        "enablement": "!authenticate"
      }
    ]
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.0",
    "@types/adm-zip": "^0.4.34",
    "@types/auth0": "^2.34.9",
    "@types/chai": "^4.2.22",
    "@types/filesystem": "0.0.32",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/request": "^2.48.7",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "axios-mock-adapter": "^1.20.0",
    "chai": "^4.3.4",
    "esbuild": "^0.14.10",
    "eslint": "^7.27.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "jest": "^27.3.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-loader": "^9.2.6",
    "typescript": "4.2.4",
    "url-loader": "^4.1.1",
    "vscode-test": "^1.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^13.1.2",
    "@aws-sdk/signature-v4-crt": "^3.45.0",
    "@razroo/razroo-angular-devkit": "^1.2.7",
    "@types/jsonwebtoken": "^8.5.5",
    "adm-zip": "^0.5.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "auth0": "^2.37.0",
    "aws-appsync": "4.1.4",
    "aws-appsync-subscription-link": "^3.0.9",
    "aws-sdk": "^2.972.0",
    "axios": "^0.21.4",
    "child_process": "^1.0.2",
    "cookiejar": "^2.1.3",
    "es6-promise": "^4.2.8",
    "file-system": "^1.2.2",
    "follow-redirects": "^1.14.4",
    "formidable": "^1.2.6",
    "graphql": "14.7.0",
    "graphql-tag": "2.11.0",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "mime": "^2.6.0",
    "mocha": "^8.4.0",
    "open": "^8.2.1",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "redux": "3.7.2",
    "request": "^2.88.2",
    "resolve": "^1.21.0",
    "subscriptions-transport-ws": "^0.9.19",
    "superagent-proxy": "^3.0.0",
    "uuid": "^8.3.2",
    "ws": "^8.2.0"
  }
}
